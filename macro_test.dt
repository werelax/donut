(define some (+ 5 5))

(let ((more (* some 10)))
  (defmacro jarl (x)
    (cons "quote" (list 1 2 (* some x)))))

(defmacro jorl (y)
  (if (> y 0)
    (cons "quote" (list 7 8 y (jorl (- 0 1))))
    1))


(jorl 1)

(jarl 10)


