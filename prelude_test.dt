(define (let_star_aux bindings body)
  (if (null? bindings)
    `(progn ,@body)
    (let ((name (car (car bindings)))
          (value (cadr (car bindings))))
      `((lambda (,name)
         ,(let_star_aux (cdr bindings) body))
        ,value))))

(defmacro (let_star_ bindings . body)
  (let_star_aux bindings body))

